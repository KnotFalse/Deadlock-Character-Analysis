{
  "name": "website-sveltekit",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "validate:graph": "node ./scripts/validate-graph.mjs",
    "test:e2e": "npm run build && playwright test",
    "test:e2e:ci": "npm run build && playwright test --reporter=list",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "fuse.js": "^7.1.0",
    "graphology": "^0.26.0",
    "graphology-shortest-path": "^2.1.0",
    "sigma": "^3.0.2"
  }
}

